<#include "presentation.ftlh"/>

<h1>Presentation Evening</h1>
<p>The Presentation Evening for the 2014/15 season was held at
		Fareham Leisure Centre on Sunday 29th March.</p>

<@section "Colts">
	<@games>
		<@match "Under-13"
			innings("Waterlooville", 122, 5, 12, "J Pearce 23", "S Reynolds 22*", "T Wallis 2/17")
			innings("Under-13 Select VI", 123, 2, 11.4, "T Wallis 29*", "C Dean 23*", "C Pratt 23*")
		/>
		<@match "Under-16"
			innings("Portsmouth & Southsea", 93, 6, 9.1, "H Gadd 49*", "M McGlinchey 2/19")
			innings("Under-16 Select VI", 94, 1, 9.5, "G Broadhurst 26*", "M McGlinchey 25*")
		/>
	</@games>
	<@awards "The Colts awards were presented by Rebecca Stemp from our
		Sponsors, Game Set and Match, accompanied by our President, Carol
		Cooper.">
		<@award "u16" "Portsmouth & Southsea" "/graphics/presentation2015/10.JPG"/>
		<@award "u13" "Waterlooville" "/graphics/presentation2015/15.JPG"/>
		<@award "u16bat" "Harry Gadd (Portsmouth & Southsea)" "/graphics/presentation2015/3.JPG"/>	
		<@award "u16bowl" "Ben Marshall (Sarisbury Athletic)" "/graphics/presentation2015/5.JPG"/>	
		<@award "u13bat" "Tom Wallis (Portsmouth)" "/graphics/presentation2015/6.JPG"/>	
		<@award "u13bowl" "Tom Wallis (Portsmouth)" "/graphics/presentation2015/9.JPG"/>	
		<@award "cfair" "Sarisbury Athletic Under-16" "/graphics/presentation2015/35.JPG"/>	
	</@awards>
</@section>

<@section "Senior">
	<@games>
		<@match "The Mayor of Fareham's VIII v Division 1 Champions' VIII"
