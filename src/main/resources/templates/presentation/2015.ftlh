		<@award "d2" "Portsmouth B", "/graphics/presentation2015/31.JPG"/>
		<@award "d3" "Bedhampton B" "/graphics/presentation2015/30.JPG"/>
		<@award "d4" "St James Casuals" "/graphics/presentation2015/28.JPG"/>
		<@award "sbat" "Nicky Wyatt (Portsmouth B)" "/graphics/presentation2015/24.JPG"/>
