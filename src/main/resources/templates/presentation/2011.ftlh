<#include "presentation.ftlh"/>

<h1>Presentation Evening</h1>
<p>The Presentation Evening for the 2010/11 season was held at
Fareham Leisure Centre on Sunday 27th March.</p>

<@section "Colts">
	<@games>
		<@match "Under-13"
			innings("Hambledon", 135, 1, 12, "R Morris 22*", "F Rockett 22*", "C Boyd 21*")
			innings("Under-13 Select VI", 127, 3, 12, "J McCoy 20*")
		/>
		<@match "Under-16"
			innings("Fareham & Crofton", 137, 6, 12, "T Kent 37", "L Cornish 25", "K Shinn 3/26")
			innings("Under-16 Select VI", 139, 1, 11.4, "J Marston 25*", "K Shinn 25*")
		/>
	</@games>
	<@awards "The Colts awards were presented by Rebecca Stemp from our sponsors Game Set & Match.">
		<@award "u16" "Fareham & Crofton" "/graphics/presentation2011/DSCF4619.JPG"/>
		<@award "u13" "Hambledon" "/graphics/presentation2011/DSCF4625.JPG"/>
		<@award "u16bat" "Matt Crouch (Petersfield)" "/graphics/presentation2011/DSCF4630.JPG"/>	
		<@award "u16bowl" "Saeed Shahid (Portsmouth & Southsea)" "/graphics/presentation2011/DSCF4633.JPG"/>	
		<@award "u13bat" "Harry Hovey (Havant)" "/graphics/presentation2011/DSCF4635.JPG"/>	
		<@award "u13bowl" "Angus Southon (Fareham & Crofton)" "/graphics/presentation2011/DSCF4637.JPG"/>	
		<@award "cfair" "Havant Under-16", "/graphics/presentation2011/DSCF4628.JPG"/>	
	</@awards>
</@section>

<@section "Senior">
	<@games>
		<@match "The Mayor of Fareham's VIII v Division 1 Champions' VIII"
